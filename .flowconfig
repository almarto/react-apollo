[ignore]
.*/node_modules/bcryptjs/.*
.*/node_modules/config-chain/.*

[options]
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable
esproposal.decorators=ignore
module.system.node.resolve_dirname=node_modules
module.system.node.resolve_dirname=src
module.name_mapper.extension='scss' -> '<PROJECT_ROOT>/__mocks__/file-mock.js'
module.name_mapper.extension='less' -> '<PROJECT_ROOT>/__mocks__/file-mock.js'
module.name_mapper.extension='css' -> '<PROJECT_ROOT>/__mocks__/file-mock.js'
module.name_mapper.extension='png' -> '<PROJECT_ROOT>/__mocks__/file-mock.js'
module.name_mapper.extension='jpg' -> '<PROJECT_ROOT>/__mocks__/file-mock.js'